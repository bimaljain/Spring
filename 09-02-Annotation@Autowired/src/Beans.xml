<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:annotation-config/>

   <bean id="spellChecker" class="com.spring.SpellChecker" />
   <bean id="textEditor1" class="com.spring.Autowired_on_SetterMethod" />
   <bean id="textEditor2" class="com.spring.Autowired_on_Properties" />
   <bean id="textEditor3" class="com.spring.Autowired_on_Constructors" />
</beans>